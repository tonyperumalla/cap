/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public TreeNode insertIntoBST(TreeNode root, int val) {
        TreeNode newNode = new TreeNode(val);
        TreeNode par =root;
        if(root == null){
            return  newNode;
        }
        TreeNode curr = root;
        while(curr!=null){
            if(val<curr.val){
                par=curr;
                curr = curr.left;
            }
            else{
                par=curr;
                curr = curr.right;
            }
            
        }
            if(val<par.val){
                par.left = newNode;
            
            }
            else{
                par.right = newNode;
            }
        



        
        return root;
        
    }
}
